{% extends 'base.html.twig' %}

{% block title %}Login Taxo Park{% endblock %}

{% block body %}
    <div class="register_form">
    <h2>Registration</h2>

    <div class="form-wrapper">
        <form method="post" class="contactForm">
            {{ form_start(form) }}
            {{ form_row(form.email) }}
            {{ form_row(form.username) }}
            {{ form_row(form.plainPassword) }}

            <button class="btn btn-lg btn-primary btn-block" formnovalidate type="submit">Register</button>
            {{ form_end(form) }}
        </form>
    </div>
    </div>

{% endblock %}
